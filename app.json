{
    "expo": {
        "name": "MovDB",
        "slug": "MovDB",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/logo512.png",
        "splash": {
            "image": "./assets/movDBsplash.png",
            "resizeMode": "contain",
            "backgroundColor": "#101010"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": false
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/logo512.png",
                "backgroundColor": "#101010"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
